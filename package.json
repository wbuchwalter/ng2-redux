    }
  ],
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/angular-redux/ng2-redux/issues"
  },
  "homepage": "https://github.com/angular-redux/ng2-redux#readme",
  "devDependencies": {
    "chai": "^3.5.0",
    "expect": "^1.8.0",
    "mocha": "^2.4.5",
    "rimraf": "^2.5.2",
    "sinon": "^1.16.1",
    "sinon-chai": "^2.8.0",
    "ts-loader": "^0.8.1",
    "ts-node": "^0.5.5",
    "typescript": "^1.8.10",
    "@angular/core": "2.0.0-rc.1",
    "es6-shim": "^0.35.0",
    "redux": "^3.4.0",
    "reflect-metadata": "0.1.3",
    "rxjs": "5.0.0-beta.6",
    "tslint": "^3.9.0",
    "zone.js": "0.6.12"
  },
